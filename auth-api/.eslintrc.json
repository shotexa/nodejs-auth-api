{
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module",
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        }
    },
    "rules": {
        "constructor-super": 2,
        "no-case-declarations": 2,
        "no-class-assign": 2,
        "no-compare-neg-zero": 2,
        "no-cond-assign": 2,
        "no-console": 0,
        "no-const-assign": 2,
        "no-constant-condition": 2,
        "no-control-regex": 2,
        "prefer-const": 2,
        "no-var": 2,
        "no-delete-var": 2,
        "no-dupe-args": 2,
        "no-dupe-class-members": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-empty-character-class": 2,
        "no-empty-pattern": 2,
        "no-empty": 2,
        "no-ex-assign": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-semi": 2,
        "semi": [
            2,
            "never"
        ],
        "no-fallthrough": 2,
        "no-func-assign": 2,
        "no-global-assign": 2,
        "no-inner-declarations": 2,
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 2,
        "no-mixed-spaces-and-tabs": 2,
        "no-obj-calls": 2,
        "no-octal": 2,
        "no-redeclare": 2,
        "no-regex-spaces": 2,
        "no-self-assign": 2,
        "no-sparse-arrays": 2,
        "no-this-before-super": 2,
        "no-undef": 2,
        "no-unexpected-multiline": 2,
        "no-unreachable": 2,
        "no-unsafe-finally": 2,
        "no-unsafe-negation": 2,
        "no-unused-labels": 1,
        "no-unused-vars": 1,
        "no-useless-escape": 2,
        "require-yield": 2,
        "use-isnan": 2,
        "quotes": [
            2,
            "single"
        ],
        "no-var": 2,
        "valid-typeof": 2,
        "no-new-symbol": 2,
        "arrow-parens": [
            2,
            "as-needed"
        ],
        "arrow-spacing": 2,
        "brace-style": 2,
        "callback-return": 2,
        "camelcase": 2,
        "capitalized-comments": 0,
        "no-return-await": 2,
        "accessor-pairs": 2,
        "array-bracket-spacing": 2,
        "block-spacing": 2,
        "comma-dangle": 2,
        "comma-spacing": 2,
        "comma-style": 0,
        "consistent-this": [
            2,
            "self"
        ],
        "curly": 0,
        "default-case": 2,
        "eqeqeq": 1
    },
    "env": {
        "browser": false,
        "node": true,
        "commonjs": true,
        "phantomjs": false,
        "es6": true
    }
}
